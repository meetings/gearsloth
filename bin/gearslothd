#!/usr/bin/env node

var config = require('../lib/config');
var daemon = require('../lib/daemon');

try {
  var conf = config.initialize(process.argv);
  if (!conf) {
    process.exit(0);
  }
  daemon.initialize(conf);
}
catch ( err )
  console.error('gearslothd:', err.message);
  process.exit(1);
}


